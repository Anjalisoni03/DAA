#include<stdio.h>
#include<limits.h>
int MCO(int c[],int i,int j)
{
  if(i==j)
  {
    return 0;
  }
  int k;
  int min=INT_MAX;
  int mul;
  for(k=i;k<j;k++)
  {
    mul=MCO(c,i,k)+MCO(c,k+1,j)+(c[i-1]*c[k]*c[j]);
    if (mul<min)
      min=mul;
  }
  return min;
}
int main()
{
  int arr[]={3,2,4,2,5};
  int n = sizeof(arr) / sizeof(arr[0]);
  printf("Minimum number of multiplications is %d ",MCO(arr,1,n-1));
  getchar();
  return 0;
}
